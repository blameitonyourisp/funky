{
    "docdash": {
        "search": true,
        "typedefs": true,
        "collapse": true,
        "sort": true,
        "scripts": ["../src/index.js", "../src/styles.css"]
    },
    "opts": {
        "destination": "./jsdoc/",
        "private": true,
        "recurse": true,
        "template": "node_modules/docdash",
        "sort": true
    },
    "plugins": [ 
        "plugins/markdown",
        "../../src/plugin/index.js"
    ],
    "markdown": {
        "hardwrap": true
    },
    "submodule": {
        
    },
    "recurseDepth": 10,
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.js(doc|x)?$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}