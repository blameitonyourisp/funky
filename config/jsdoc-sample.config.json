{
    "opts": {
        "destination": "./jsdoc-sample/",
        "private": true,
        "recurse": true,
        "sort": true
    },
    "plugins": [ 
        "plugins/markdown",
        "."
    ],
    "markdown": {
        "hardwrap": true
    },
    "recurseDepth": 10,
    "source": {
        "include": ["sample"],
        "includePattern": ".+\\.js(doc|x)?$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}