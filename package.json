{
    "name": "@13ms/funky",
    "version": "1.0.4",
    "description": "A jsdoc plugin for quickly creating scoped and access tagged functions",
    "type": "module",
    "main": "dist/bundle.min.cjs",
    "scripts": {
        "test": "jest",
        "build": "rollup --config ./config/rollup.config.js",
        "docs": "jsdoc -c ./config/jsdoc.config.json",
        "docs-sample": "jsdoc -c ./config/jsdoc-sample.config.json",
        "serve-docs": "npm run docs && http-server ./jsdoc",
        "serve-docs-sample": "npm run docs-sample && http-server ./jsdoc-sample"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/blameitonyourisp/funky.git"
    },
    "keywords": [
        "jsdoc",
        "plugin"
    ],
    "author": "James Reid",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/blameitonyourisp/funky/issues"
    },
    "homepage": "https://github.com/blameitonyourisp/funky#readme",
    "devDependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.21.2",
        "@jest/globals": "^29.5.0",
        "@rollup/plugin-terser": "^0.4.0",
        "docdash": "^2.0.1",
        "http-server": "^14.1.1",
        "jest": "^29.5.0",
        "rollup": "^3.20.2"
    },
    "dependencies": {
        "jsdoc": "^4.0.2"
    }
}
